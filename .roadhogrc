{
  "entry": "src/index.js",
  "proxy": {
    "/api": {
      "target": "http://crm.zhushi/api/",
      "changeOrigin": true,
      "pathRewrite": { "^/api" : "" }
    },
    "/zhushi":{
      "target": "http://crm.zhushi/",
      "changeOrigin": true,
      "pathRewrite": { "^/zhushi" : "" }
    }
  },
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime",
		    ["import", { "libraryName": "antd", "style": "css" }]
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime",
		    ["import", { "libraryName": "antd", "style": "css" }]
      ]
    }
  },
  "theme": "src/theme.js"
}
